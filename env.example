# Spotify Configuration
# Copy this file to .env.local and fill in your actual values

# Your Spotify App Client ID (from Spotify Developer Dashboard)
REACT_APP_SPOTIFY_CLIENT_ID=your_spotify_client_id_here

# Your Spotify App Client Secret (keep this secure!)
REACT_APP_SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Your Spotify Playlist ID (from playlist URL)
REACT_APP_SPOTIFY_PLAYLIST_ID=your_playlist_id_here

# Redirect URI for Spotify OAuth (update for production)
REACT_APP_SPOTIFY_REDIRECT_URI=http://localhost:3000

# Development/Production Environment
NODE_ENV=development
