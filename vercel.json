{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_SPOTIFY_CLIENT_ID": "974425ac66744974a11a42a9c9c9608f",
    "REACT_APP_SPOTIFY_CLIENT_SECRET": "879bec48ef484cd1926625b5634cda17"
  }
}
